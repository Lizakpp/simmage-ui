{
  "name": "simmage-ui",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
      "build:de": "ng build --no-progress --output-path=dist/de -aot -prod --bh /de/ --i18n-file=src/i18n/messages.de.xlf --i18n-format=xlf --locale=de",
    "build:en": "ng build --no-progress --output-path=dist/en -aot -prod --bh /en/ --i18n-file=src/i18n/messages.en.xlf --i18n-format=xlf --locale=en",
    "build:fr": "ng build --no-progress --output-path=dist/fr -aot -prod --bh /fr/ --i18n-file=src/i18n/messages.fr.xlf --i18n-format=xlf --locale=fr",
    "build:all": "concurrently \"npm run build:de\" \"npm run build:en\" \"npm run build:fr\"",
    "extract-i18n": "ng xi18n --output-path src/i18n --locale en && xliffmerge -v --profile xliffmerge.json",
    "merge": "xliffmerge -v --profile xliffmerge.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": ">=4.1.0-beta <5.0.0",
    "@angular/common": ">=4.1.0-beta <5.0.0",
    "@angular/compiler": ">=4.1.0-beta <5.0.0",
    "@angular/core": ">=4.1.0-beta <5.0.0",
    "@angular/forms": ">=4.1.0-beta <5.0.0",
    "@angular/http": ">=4.1.0-beta <5.0.0",
    "@angular/material": "2.0.0-beta.3",
    "@angular/platform-browser": ">=4.1.0-beta <5.0.0",
    "@angular/platform-browser-dynamic": ">=4.1.0-beta <5.0.0",
    "@angular/router": ">=4.1.0-beta <5.0.0",
    "ag-grid": "^9.0.0",
    "ag-grid-angular": "^9.0.0",
    "angular-calendar": "^0.13.5",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "rxjs": "^5.1.0",
    "zone.js": "^0.7.6"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0",
    "@angular/compiler-cli": ">=4.1.0-beta <5.0.0",
    "@angular/language-service": ">=4.1.0-beta <5.0.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "concurrently": "^3.4.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ngx-i18nsupport": "^0.2.3",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.4.2",
    "typescript": "~2.1.0"
  }
}
